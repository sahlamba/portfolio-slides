    "@fortawesome/react-fontawesome": "^0.1.4",
    "@fullpage/react-fullpage": "^0.1.10",
    "@material-ui/core": "^3.8.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-vertical-timeline-component": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.6",

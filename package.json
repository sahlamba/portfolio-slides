    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "linters": {
